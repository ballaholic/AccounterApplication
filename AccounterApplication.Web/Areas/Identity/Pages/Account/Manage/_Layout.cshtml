@using Resources = AccounterApplication.Common.LocalizationResources.Views.Identity.IdentityResources;

@{
    Layout = "/Views/Shared/_Layout.cshtml";
}

<h1>@Resources.ManageYourAccount</h1>

<div>
    <h4>@Resources.ChangeAccountSettings</h4>
    <hr />
    <div class="row">
        <div class="col-md-3">
            <partial name="_ManageNav" />
        </div>
        <div class="col-md-9">
            @RenderBody()
        </div>
    </div>
</div>

@section Scripts {
    @RenderSection("Scripts", required: false)
}
